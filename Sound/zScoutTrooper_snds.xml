<!-- converted from XMB to XML -->

<protounitsounddef>
	<protounit name="zScoutTrooper">
		<soundtype name="Select">
			<soundset name="ScoutTrooperSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="StormtrooperGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="StormtrooperDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="ScoutTrooperSpawn"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="ScoutTrooperMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="ScoutTrooperAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>