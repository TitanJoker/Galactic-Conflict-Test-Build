<!-- converted from XMB to XML -->

<protounitsounddef>
	<protounit name="zDemolitionTrooper">
		<soundtype name="Select">
			<soundset name="DemoTrooperSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="StormtrooperGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="StormtrooperDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="DemoTrooperSpawn"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="DemoTrooperMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="DemoTrooperAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>