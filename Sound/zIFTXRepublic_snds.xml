<!-- converted from XMB to XML -->

<protounitsounddef>
	<protounit name="zATRT">
		<soundtype name="Select">
			<soundset name="ATRTSelect"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="TankDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="ATRTSpawn"/>
		</soundtype>
		<soundtype name="Exists">
			<soundset name="x"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="ATRTMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="ATRTAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
