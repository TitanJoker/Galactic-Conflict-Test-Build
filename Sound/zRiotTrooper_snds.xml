<!-- converted from XMB to XML -->

<protounitsounddef>
	<protounit name="zRiotTrooper">
		<soundtype name="Select">
			<soundset name="RiotTrooperSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="StormtrooperGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="StormtrooperDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="RiotTrooperSpawn"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="RiotTrooperMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="RiotTrooperAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>