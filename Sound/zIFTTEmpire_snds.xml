<!-- converted from XMB to XML -->

<protounitsounddef>
	<protounit name="zIFTTEmpire">
		<soundtype name="Select">
			<soundset name="IFTXSelect"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="TankDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="IFXTSpawn"/>
		</soundtype>
		<soundtype name="Exists">
			<soundset name="RepulsorMediumExist"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="IFTXMove"/>
				</choice>
				<choice name="enemy">
					<soundset name="IFTXAttack"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>